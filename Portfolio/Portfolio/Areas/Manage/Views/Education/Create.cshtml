@model Education
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Manage/Views/Shared/_ManageLayout.cshtml";
}

<div class="container-fluid">
    <form method="post">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="SchoolName">SchoolName</label>
                <input asp-for="SchoolName" class="form-control" placeholder="SchoolName">
            </div>
            <div class="form-group col-md-6">
                <label asp-for="Degree">Degree</label>
                <input asp-for="Degree" class="form-control" placeholder="Degree">
            </div>
            <div class="form-group col-md-6">
                <label asp-for="PresentTime">PresentTime</label>
                <input asp-for="PresentTime" class="form-control" placeholder="PresentTime">
            </div>
            <div class="form-group col-md-6">
                <label asp-for="PastTime">PastTime</label>
                <input asp-for="PastTime" class="form-control" placeholder="PastTime">
            </div>
            <div class="form-group col-md-6">
                <label asp-for="EducationName">EducationName</label>
                <input asp-for="EducationName" class="form-control" placeholder="EducationName">
            </div>
        </div>
        <div class="form-row">

            <div class="form-group col-md-4">
                <label asp-for="GpaDegree">GpaDegree</label>
                <input asp-for="GpaDegree" class="form-control" placeholder="GpaDegree">
            </div>


        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>
    </form>
</div>

