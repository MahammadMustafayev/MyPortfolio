@model Skill
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Manage/Views/Shared/_ManageLayout.cshtml";
}

<div class="container-fluid">
    <form method="post">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="Icon">Icon</label>
                <input asp-for="Icon" class="form-control" placeholder="Icon">
            </div>
        </div>
        <div class="form-row">

            <div class="form-group col-md-4">
                <label asp-for="WorkFlow">WorkFlow</label>
                <input asp-for="WorkFlow" class="form-control" placeholder="WorkFlow">
            </div>


        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>
    </form>
</div>

